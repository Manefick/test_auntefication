@model List<WorkStock>
<div class="bg-primary m-1 p-1 text-white"><h4>Work Stock</h4></div>
<table>
    <tr><th>Name Tabaco</th><th>Tabaco Weigth</th><th>CompanyId</th><th>HookahMaster</th><th>Количевсто принятых пачек</th></tr>
    @foreach (WorkStock workStock in Model)
    {
        <tr>
            <td>@workStock.NameTabaco</td>
            <td>@workStock.TabacoWeigh</td>
            <td>@workStock.CompanyId</td>
            <td>@workStock.Data</td>
            <td>@workStock.HookahMaster</td>
            <td>@workStock.CountTabacoPack</td>
        </tr>
    }
</table>
<a class="btn btn-primary" asp-controller="StockReplenishment" role="button" asp-action="AddTabacoToStock">Добавить табак на склад</a>
<a class="btn btn-primary" asp-controller="StockReplenishment" role="button" asp-action="AddTabacoToWorkStock">Принять табак</a>
<a class="btn btn-primary" asp-controller="Display" role="button" asp-action="ShowStock">Склад</a>
<a class="btn btn-primary" asp-controller="Rediscount" role="button" asp-action="RedisCount">Переучет</a>
