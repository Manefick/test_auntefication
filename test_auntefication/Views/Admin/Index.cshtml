@model IEnumerable<IdentityUser>
@{ Layout = null;}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Succes</title>
</head>
<body>
    <table>
        <tr><th>ID</th><th>Name</th><th>Email</th></tr>
        @if(Model.Count() == 0)
        {
            <tr><td>No user</td></tr>
        }
        else {
            foreach (IdentityUser user in Model) {
    <tr>
        <td>@user.Id</td>
        <td>@user.UserName</td>
        <td>@user.Email</td>
    </tr>}
        }
    </table>
</body>
</html>

