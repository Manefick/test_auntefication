@model Dictionary<string, Dictionary<string,string>>
@{ Layout = null;}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Succes</title>
</head>
<body>
    <table>
        <tr><th>ID</th><th>Name</th><th>Email</th><th>Role</th></tr>
        @if(Model.Count() == 0)
        {
            <tr><td>No user</td></tr>
        }
        else {
            foreach (KeyValuePair<string, Dictionary<string,string>> keyValue in Model) {
    <tr>
        <td>@keyValue.Value["Id"]</td>
        <td>@keyValue.Value["Name"]</td>
        <td>@keyValue.Value["Email"]</td>
        <td>@keyValue.Value["Role"]</td>
    </tr>}
        }
    </table>
</body>
</html>

